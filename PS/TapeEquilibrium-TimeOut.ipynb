    "def solution(A):\n",
    "    length=len(A)\n",
    "    int answer\n",
    "    while(length>0):\n",
    "        len1=A[:length]\n",
    "        len2=A[length:]\n",
    "        val1=sum(len1,0.0)\n",
    "        val2=sum(len2,0.0)\n",
    "        result=abs(val1-val2)\n",
    "        if(answer>result):\n",
    "            answer=result\n",
    "    return answer\n",
    "    pass"
   ]
  }
